#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules/jest-expo/bin/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules/jest-expo/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules/jest-expo/bin/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules/jest-expo/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/jest-expo@53.0.9_@babel+core@7.28.0_expo@53.0.20_@babel+core@7.28.0_@expo+metro-runtime_21ccb3254291e6b972fd0139d22086ee/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
