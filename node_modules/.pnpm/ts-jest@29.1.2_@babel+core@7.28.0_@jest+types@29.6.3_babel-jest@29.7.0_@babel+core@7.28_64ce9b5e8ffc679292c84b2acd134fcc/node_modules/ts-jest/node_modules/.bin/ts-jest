#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.28.0_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.28_64ce9b5e8ffc679292c84b2acd134fcc/node_modules/ts-jest/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.28.0_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.28_64ce9b5e8ffc679292c84b2acd134fcc/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.28.0_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.28_64ce9b5e8ffc679292c84b2acd134fcc/node_modules/ts-jest/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.28.0_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.28_64ce9b5e8ffc679292c84b2acd134fcc/node_modules:/Users/volodymyrgry/project/intelligent_finance_tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
